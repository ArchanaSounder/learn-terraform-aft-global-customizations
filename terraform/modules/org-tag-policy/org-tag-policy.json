{
    "tags": {
      "Environment": {
        "tag_key": {
          "@@assign": "Environment"
        },
        "tag_value": {
          "@@assign": [
            "Production",
            "UAT",
            "DEV",
            "Non-Production"
          ]
        },
        "enforced_for": {
          "@@assign": [
            "dynamodb:*",
            "ec2:instance",
            "ec2:volume",
            "elasticloadbalancing:*",
            "lambda:*"
          ]
        }
      }
    }
  }
